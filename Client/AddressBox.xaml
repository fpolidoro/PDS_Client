<UserControl x:Class="Client.AddressBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Client"
             mc:Ignorable="d" 
             d:DesignHeight="18" d:DesignWidth="140">
    <Grid>
        <TextBox x:Name="textBox" Margin="0,0,0,0" TextWrapping="Wrap" TextChanged="textBox_TextChanged" FocusManager.IsFocusScope="True" DataObject.Pasting="DataObject_OnPasting" PreviewKeyDown="textBox_PreviewKeyDown" MaxLength="256"/>

    </Grid>
</UserControl>
